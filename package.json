{
  "name": "mongoose-plugins-recurrency",
  "version": "0.1.4",
  "description": "This package was created so some mongoose plugins could be installed only  once per schema. And not every time a new model is created using that same schema.",
  "main": "src/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "node_modules/.bin/mocha --reporter spec",
    "cover": "node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- -R spec test/*"
  },
  "engines": {
    "node": ">=4.4.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jorgecuesta/mongoose-plugins-recurrency.git"
  },
  "keywords": [
    "mongoose",
    "mongodb",
    "mongo",
    "mongoose-plugins"
  ],
  "author": "Jorge S. Cuesta <jckpo2@gmail.com>",
  "contributors": [
    "Jeffrey Soriano <jeffreysoriano5@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jorgecuesta/mongoose-plugins-recurrency/issues"
  },
  "homepage": "https://github.com/jorgecuesta/mongoose-plugins-recurrency#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "istanbul": "^0.4.3",
    "mocha": "^2.5.3",
    "mocha-mongoose": "^1.2.0",
    "mongoose": "^4.4.19",
    "mongoose-multitenancy": "^0.1.4",
    "should": "^2.1.1",
    "travis-cov": "^0.2.5"
  },
  "peerDependencies": {
    "mongoose": ">=4.x.x"
  },
  "dependencies": {
    "lodash": "^4.13.1",
    "recursive-diff": "^0.1.2"
  }
}
